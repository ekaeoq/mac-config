# Example aliases
# alias for accesing the cloud drive for uni
alias faksnow="cd ~/Library/Mobile\ Documents/com\~apple\~CloudDocs/faks/"
# alias for accesing the conda environment
alias condanow="conda config --set auto_activate_base True
source ~/.zshrc"
# alias for STOPING access to the conda environment
alias condamove="conda config --set auto_activate_base False
source ~/.zshrc"
### aliases for QA-labwork
alias nixd="nix develop"
alias flubuild="flutter build web --dart-define-from-file config_local.json"
alias flurun="flutter run -d chrome --dart-define-from-file=config_local.json"

